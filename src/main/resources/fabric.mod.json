{
	"schemaVersion": 1,
	"id": "genadom00001",
	"version": "${version}",
	"name": "Minecraft_Webpage_Viewer",
	"description": "This Client-sided Minecraft Fabric mod will allow you to display specified webpages in item frame maps, with plenty of configuration opportunities!",
	"authors": [
		"Dominic Gena!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/genadom00001/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "com.genadom.genadom00001.MinecraftWebpageViewer"
		],
		"fabric-datagen": [
			"com.genadom.genadom00001.MinecraftWebpageViewerDataGenerator"
		],
		"client": [
			"com.genadom.genadom00001.MinecraftWebpageViewerClient"
		]
	},
	"mixins": [
		"genadom00001.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}